<form
  action="/country/{{Country._id}}/editcountry"
  method="POST"
  enctype="multipart/form-data"
>
  <label for="name">Name:
    <input type="text" id="name" name="name" />
  </label>
  <label for="image">
    <input type="file" name="image" accept="image/png, image/jpg" />
  </label>
  <label for="location">Location:
    <input type="text" id="name" name="location" />
  </label>
  <label for="population">Country:
    <input type="text" id="population" name="population" />
  </label>
  <button type="submit">Edit City</button>
</form>