<form
  action="/country/{{Country._id}}/addcity"
  method="POST"
  enctype="multipart/form-data"
>
  <label for="name">Name:
    <input type="text" id="name" name="name" />
  </label>
  <label for="image">
    <input type="file" name="image" accept="image/png, image/jpg" />
  </label>
  <label for="location">Location:
    <input type="text" id="name" name="location" />
  </label>
  <label for="country">Country:
    <input
      type="text"
      id="country"
      name="country"
      value={{Country.name}}
      disabled="disabled"
    />
  </label>
  <button type="submit">Add the City</button>
</form>